{
    "manifest_version": 3,
    "name": "Vision Tab",
    "version": "0.1",
    "description": "New tab for chrome for programmers to keep them focused and motivated",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks"
    ],
    "icons": {
        "16": "./src/fav.png",
        "32": "./src/fav.png",
        "48": "./src/fav.png",
        "128": "./src/fav.png"
    }
}
